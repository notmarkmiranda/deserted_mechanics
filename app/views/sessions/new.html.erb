<%= form_with url: sign_in_path do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email, autofocus: true %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "Sign In" %>
<% end %>
<%= link_to "Forgot your password?", forgot_password_path %>
